{
  "name": "turbo-next-nest-graphql-prisma",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "pnpm prisma generate",
    "build": "FORCE_COLOR=2 turbo run build",
    "dev": "FORCE_COLOR=2 turbo run dev --parallel",
    "lint": "FORCE_COLOR=2 turbo run lint",
    "format": "FORCE_COLOR=2 turbo run format",
    "build:client": "FORCE_COLOR=2 pnpm turbo run --filter=\"@org/client\" build",
    "start:client": "FORCE_COLOR=2 pnpm --filter=\"@org/client\" start",
    "build:server": "FORCE_COLOR=2 pnpm turbo run --filter=\"@org/server\" build",
    "start:server": "FORCE_COLOR=2 pnpm --filter=\"@org/server\" start"
  },
  "dependencies": {
    "@prisma/client": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "crlf-phobia": "^1.0.1",
    "prettier": "^2.7.1",
    "prisma": "^4.5.0",
    "prisma-nestjs-graphql": "^17.0.1",
    "turbo": "^1.6.2",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16.17.0"
  }
}